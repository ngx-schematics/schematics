{
  "name": "@ngx-schematics/husky",
  "version": "0.0.0",
  "description": "A schematic for adding Husky git hooks to a project",
  "scripts": {
    "prebuild": "dtsgen src/husky/schema.json -o src/husky/options.d.ts",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.1.1",
    "@angular-devkit/schematics": "^9.1.1",
    "schematics-utilities": "^2.0.1",
    "typescript": "~3.8.2"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "dtsgenerator": "^2.5.0",
    "husky": "^4.2.5",
    "jasmine": "^3.5.0"
  }
}
